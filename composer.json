{
  "name": "kenkataiwa/doctrine-migrations-module",
  "description": "Doctrine Migrations module for Zend Framework 2",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "zf2",
    "zend",
    "module",
    "doctrine",
    "migrations"
  ],
  "homepage": "http://www.doctrine-project.org/",
  "minimum-stability": "dev",
  "stability": "dev",
  "authors": [
    {
      "name": "Kenneth Kataiwa",
      "email": "mail@kenkataiwa.com"
    }
  ],
  "require": {
    "php": ">=5.3.3",
    "zendframework/zend-modulemanager": "2.*",
    "zendframework/zend-eventmanager": "2.*",
    "zendframework/zend-servicemanager": "2.*",
    "doctrine/doctrine-orm-module": "0.8.*",
    "doctrine/migrations": "1.0.*@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "3.7.*",
    "murganikolay/doctrine-migrations-module": "dev-master",
    "doctrine/doctrine-migrations-bundle": "dev-master"
  },
  "autoload": {
    "psr-0": {
      "DoctrineMigrationsModule\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
